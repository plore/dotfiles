{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "diffEditor.codeLens": true,
  "diffEditor.maxComputationTime": 0,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.minimap.enabled": false,
  "editor.multiCursorLimit": 100000,
  "editor.stickyScroll.enabled": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "git.closeDiffOnOperation": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "git.timeline.showAuthor": false,
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "scminput": false
  },
  "gitlens.blame.avatars": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "playwright.reuseBrowser": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.indexing": true,
  "python.analysis.stubPath": "",
  "python.venvFolders": ["Library/Caches/pypoetry/virtualenvs"],
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "Inconsolate for Powerline",
  "testExplorer.useNativeTesting": true,
  "window.titleBarStyle": "native",
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#4f545c",
    "tab.activeBorder": "#ffffff"
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 12,
  "workbench.editor.revealIfOpen": true,
  "workbench.preferredLightColorTheme": "Default Dark+",
  "workbench.tree.indent": 12
}
